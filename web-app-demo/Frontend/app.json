{
    "name": "web-app-demo",
    "env": {
        "BACKEND_URL": {
            "description": "specify the backend url you deployed to",
            "required": true
        },
        "PROJECT_ID": {
            "description": "provide a project id",
            "required": true
        }
    },
    "hooks": {
        "prebuild": {
            "commands": [
                "cd ../Backend/","gcloud run deploy backend --source  --project=$PROJECT_ID --region=$REGION"
            ]
        }
    }
}