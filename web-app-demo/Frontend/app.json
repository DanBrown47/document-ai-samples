{
    "name": "web-app-demo",
    "env": {
        "BACKEND_URL": {
            "description": "specify the backend url you deployed to",
            "required": true
        }
    },
    "hooks": {
        "prebuild": {
            "commands": [
                "cloudshell_open --repo_url 'https://github.com/galz10/document-ai-samples.git' --dir 'web-app-demo/Backend' --page 'shell' --git_branch 'main' --force_new_clone"
            ]
        }
    }
}